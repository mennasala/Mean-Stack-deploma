    <div class="mb-3">
      <input class="form-control" name="bookName" placeholder="book name" />
    </div>
    <div class="mb-3">
      <input class="form-control" name="bookWriter" placeholder="book writer" />
    </div>
    <div class="mb-3">
      <input class="form-control" name="department" placeholder="department" />
    </div>
    <div class="mb-3">
      <input
        class="form-control"
        name="numberOfPages"
        placeholder="number of pages"
      />
